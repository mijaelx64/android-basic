<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.possiblelabs.guishowcase">

    <uses-permission android:name="android.permission.INTERNET"/>

    <application android:allowBackup="true" android:label="@string/app_name"
        android:icon="@mipmap/ic_launcher" android:theme="@style/AppTheme">

        <activity android:name=".MainActivity" android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".AlertDialogActivity"/>
        <activity android:name=".AutoCompleteActivity"/>
        <activity android:name=".ButtonActivity"/>
        <activity android:name=".CheckboxActivity"/>
        <activity android:name=".DatePickerActivity"/>
        <activity android:name=".GalleryActivity"/>
        <activity android:name=".ListViewActivity"/>
        <activity android:name=".RadioButtonActivity"/>
        <activity android:name=".RatingsBarActivity"/>
        <activity android:name=".SpinnerActivity"/>
        <activity android:name=".TextViewActivity"/>
        <activity android:name=".TimePickerActivity"/>
        <activity android:name=".ToggleButtonActivity"/>
        <activity android:name=".ViewPagerActivity"/>
        <activity android:name=".WebViewActivity"/>

    </application>

</manifest>
